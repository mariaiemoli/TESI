[fractureData2]

spaceDimension = 1.

        [./levelSet]

				levelSet = y-x-0.1
                ylevelSet = y-0.31-0.69*t
				xlevelSet = x-0.21-0.69*t
                levelSetCut = -1
                yMap = 0.31+0.69*t
				xMap = 0.21+0.69*t
				
                normalMap = [1.,-1.]


        [../]

        [./domain]


                thickness = 0.012
				
