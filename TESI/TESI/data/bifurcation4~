%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% parameters for program laplacian    PARAMETRIZZATO                      %
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

meshFile = meshes/mesh
folderVTK = ./vtk/

numberFractures = 1

[dataTime]

        endTime = 0.1
        dt = 0.002
        
[../]

[mediumData]

        [./domain]

                meshExternal = none
                meshFolder = ./meshes/

                spatialDiscretization = 21
                spatialInclination = 0.
                lengthAbscissa = 300.
                lengthOrdinate = 300.
                lengthQuota = 300.
                meshType = GT_PK(2,1)
                spaceDimension = 2.

        [../]

[../]



[fractureData0]

spaceDimension = 1.

        [./levelSet]
			
			levelSet = y+x-0.52
			ylevelSet = y-0.31+0.31*t
			xlevelSet = x-0.21-0.31*t          
			levelSetCut = -1
			yMap = 0.31-0.31*t
			xMap = 0.21+0.31*t
		
	        integrationTypeSimplex = IM_STRUCTURED_COMPOSITE(IM_TRIANGLE(3),1)

        [../]

        [./domain]

	        thickness = 0.013
	
	        spacing = x+0.21
	
			a = 0.21
			b = 0.52			
	        spatialDiscretization = 81
	        translateAbscissa = 0.21
	        lengthAbscissa = 0.31 #0.5
	        lengthOrdinate = 0.
	        lengthQuota = 0.
	        meshType = GT_PK(1,1)
	
        [../]

        [./saturation]

			numberFlux = 1
			x0 = 0.
            u0 = 0. #1.*(x<0.31)  # 0.25-0.25*(x<0.)  # 1.-1.*(x<0.)  #1.*(x>0.)  
			invPorosity = 1.

			[./fluxData0]	# sinistra
			
				f = x*(1-x)
				f1 = 1-2*x  
				h = 3
				us = 0.5 
				us_meno = -0.7712
				us_piu = 1.7712
				bc1 = 0.2
				bc2 = 0.2
				
			[../]
			
        [../]


[../]


[fractureData1]

spaceDimension = 1.

        [./levelSet]
			
			levelSet = y-3.1*x+0.341
		    ylevelSet = y-0.31*t
			xlevelSet = x-0.11-0.1*t            
			levelSetCut = -1
			yMap = 0.31*t
			xMap = 0.11+0.1*t
		
	        integrationTypeSimplex = IM_STRUCTURED_COMPOSITE(IM_TRIANGLE(3),1)

        [../]

        [./domain]


                thickness = 0.011

                spacing = x+0.11
                
                a = 0.11
                b = 0.21

                spatialDiscretization = 81
                translateAbscissa = 0.11
                lengthAbscissa = 0.1 #0.5
                lengthOrdinate = 0.
                lengthQuota = 0.

                meshType = GT_PK(1,1)


        [../]

        [./saturation]

			numberFlux = 1
			x0 = 0.
            u0 = 0. # 1.*(x>0.15)  # 0.25-0.25*(x<0.)  # 1.-1.*(x<0.)  #1.*(x>0.)  
			invPorosity = 1.

			[./fluxData0]	# sinistra
			
				f = -x*x*(1.-x)*(1.-x)*0.5/(x*x+(1.-x)*(1.-x))
				f1 = x*(-0.5*x^4+1.25*x^3-1.5*x^2+x-0.25)/(x^2-x+0.5)^2  
				h = 2
				us = 0.5 
				us_meno = -0.7712
				us_piu = 1.7712
				bc1 = 1.
				bc2 = 0.
				
			[../]
			
			[./fluxData1] # destra
			
				f = -x*x*(1.-x)*(1.-x)/(x*x+(1.-x)*(1.-x))
				f1 = -2*x*(2*x^4-5*x^3+6*x^2-4*x+1)/(2*x^2-2*x+1)^2  
				h = 2
				us = 0.5
				us_meno = 0.2659
				us_piu =  0.7341
				bc1 = 0.5
				bc2 = 0.25
				
			[../]
			
        [../]

[../]

[fractureData2]

spaceDimension = 1.

        [./levelSet]

		levelSet = y-x-0.1
                ylevelSet = y-0.31-0.69*t
		xlevelSet = x-0.21-0.69*t
                levelSetCut = -1
                yMap = 0.31+0.69*t
		xMap = 0.21+0.69*t

                integrationTypeSimplex = IM_STRUCTURED_COMPOSITE(IM_TRIANGLE(3),1)

        [../]

        [./domain]


                thickness = 0.012

                spacing = x+0.21  %here the fracture starts at x=0.1 
                
                a = 0.21
                b = 0.9
                spatialDiscretization = 81 
                translateAbscissa = 0.21
                lengthAbscissa = 0.69 #1.
                lengthOrdinate = 0.
                lengthQuota = 0.

                meshType = GT_PK(1,1)

        [../]

        [./saturation]

			numberFlux = 1
			x0 = 0.
            u0 = 0. #1.  # 1.*(x<0.3)  # 0.25-0.25*(x<0.)  # 1.-1.*(x<0.)  #1.*(x>0.)  
			invPorosity = 1.

			[./fluxData0]	# sinistra
			
				f = -x*x*(1.-x)*(1.-x)*0.5/(x*x+(1.-x)*(1.-x))
				f1 = x*(-0.5*x^4+1.25*x^3-1.5*x^2+x-0.25)/(x^2-x+0.5)^2  
				h = 2
				us = 0.5 
				us_meno = -0.7712
				us_piu = 1.7712
				bc1 = 1.
				bc2 = 0.
				
			[../]
			
			[./fluxData1] # destra
			
				f = -x*x*(1.-x)*(1.-x)/(x*x+(1.-x)*(1.-x))
				f1 = -2*x*(2*x^4-5*x^3+6*x^2-4*x+1)/(2*x^2-2*x+1)^2  
				h = 2
				us = 0.5
				us_meno = 0.2659
				us_piu =  0.7341
				bc1 = 0.
				bc2 = 0.25
				
			[../]
			
        [../]

[../]
